    "collapsible_content_mwniwH": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible_row_XFkWqF": {
          "type": "collapsible_row",
          "settings": {
            "heading": "What is included with enrollment?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>Enrollment includes access to all scheduled classes, ensembles, workshops, special events, housing, meals, and the Vic Firth factory<br/>experience.</p>",
            "page": ""
          }
        },
        "collapsible_row_fqLwVa": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Who is the Vic Firth Summer Institute designed for?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>The Vic Firth Summer Institute is designed for percussionists 18 years of age and older, including advanced students, college-level musicians, and emerging professionals seeking focused, high-level<br/>instruction.</p>",
            "page": ""
          }
        },
        "collapsible_row_gVpM8W": {
          "type": "collapsible_row",
          "settings": {
            "heading": "What styles and areas of percussion arecovered?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>The institute primarily focuses on  orchestral and chamber percussion, with additional exploration of contemporary techniques, ensemble performance, and professional development topics.</p>",
            "page": ""
          }
        },
        "collapsible_row_YgA7Gk": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Where does the institute take place?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>The Vic Firth Summer Institute is held on the campus of the University of Maine.</p>",
            "page": ""
          }
        },
        "collapsible_row_eEnBat": {
          "type": "collapsible_row",
          "settings": {
            "heading": "What does a typical day look like?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>Each day features a thoughtfully balanced schedule of performance classes, ensemble rehearsals, workshops, and faculty-led sessions, allowing for both intensive study and collaborative learning.</p>",
            "page": ""
          }
        },
        "collapsible_row_H4NtUy": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Who will be teaching at the institute?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>Participants study with an accomplished faculty of professional percussionists and educators, including Mike Gould, Bonnie<br/>Whiting, Clara Warnaar, Terry Longshore, and Mike Competello.</p>",
            "page": ""
          }
        },
        "collapsible_row_im7JKg": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Is there a Vic Firth factory visit included?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>Yes. Enrollment includes an exclusive visit to the Vic Firth factory, offering articipants insight into the craftsmanship and design behind Vic Firth sticks and mallets.</p>",
            "page": ""
          }
        },
        "collapsible_row_ffTRem": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Is housing and food included?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>Yes. Housing and meals are provided for all enrolled participants throughout the duration of the institute.</p>",
            "page": ""
          }
        },
        "collapsible_row_DyM9Hi": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Do I need to bring my own instruments andequipment?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>No, participants do not need to bring their own instruments.  Everything needed for the week will be provided, including sticks and mallets.</p>",
            "page": ""
          }
        },
        "collapsible_row_FERKJ6": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Is an audition required to attend?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>No. An audition is not required to attend the Vic Firth Summer Institute.</p>",
            "page": ""
          }
        },
        "collapsible_row_9AHmb6": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Is travel included?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>No. Travel to and from the University of Maine is the responsibility of the attendee.</p>",
            "page": ""
          }
        },
        "collapsible_row_YVCWPw": {
          "type": "collapsible_row",
          "settings": {
            "heading": "How do I register?",
            "icon_text": "",
            "icon": "check_mark",
            "image_caption": "",
            "row_content": "<p>Registration details and instructions can be found on this page. Space is limited and early registration is encouraged.</p>",
            "page": ""
          }
        }
      },
